CONFIG
 ParameterHomotopy: 2; % move to specific parameter values
END;
INPUT
 variable_group x,y,z;
 hom_variable_group l0,l1;
 function f,G1,G2,G3;
 parameter xs,ys,zs,eps; % (xs,ys) is point measuring distance from and eps is perturbation

 f = 64*(x^2-1)*(y^2-1)*((x+y)^2 - 2)*((x-y)^2-2) - (-4*(1+1.414214)*((x^2+y^2)^2) +( 8*(2+1.414214)*z^2 +2*(2+7*1.414214*1 ))*(x^2+y^2) -16*z^4 + 8*(1-2*1.414214)*z^2*1 - (1+12*1.414214))^2 - eps;

G1 = l0*(x - xs) + l1*(2.0*(2.0*x*(27.31370849898475938744013546966*z^2 + 23.798989873223330704377076472156) - 38.62741699796951877488027093932*x*(x^2 + y^2))*(9.6568542494923796937200677348301*(x^2 + y^2)^2 - 1.0*(x^2 + y^2)*(27.31370849898475938744013546966*z^2 + 23.798989873223330704377076472156) + 14.627416997969522327593949739821*z^2 + 16.0*z^4 + 17.970562748477142633873882004991) + (64.0*x^2 - 64.0)*(y^2 - 1.0)*(2.0*x + 2.0*y)*((x - 1.0*y)^2 - 2.0) + 128.0*x*((x + y)^2 - 2.0)*(y^2 - 1.0)*((x - 1.0*y)^2 - 2.0) + (64.0*x^2 - 64.0)*((x + y)^2 - 2.0)*(y^2 - 1.0)*(2.0*x - 2.0*y));
G2 = l0*(y - ys) + l1*(2.0*(2.0*y*(27.31370849898475938744013546966*z^2 + 23.798989873223330704377076472156) - 38.62741699796951877488027093932*y*(x^2 + y^2))*(9.6568542494923796937200677348301*(x^2 + y^2)^2 - 1.0*(x^2 + y^2)*(27.31370849898475938744013546966*z^2 + 23.798989873223330704377076472156) + 14.627416997969522327593949739821*z^2 + 16.0*z^4 + 17.970562748477142633873882004991) + 2.0*y*(64.0*x^2 - 64.0)*((x + y)^2 - 2.0)*((x - 1.0*y)^2 - 2.0) + (64.0*x^2 - 64.0)*(y^2 - 1.0)*(2.0*x + 2.0*y)*((x - 1.0*y)^2 - 2.0) - 1.0*(64.0*x^2 - 64.0)*((x + y)^2 - 2.0)*(y^2 - 1.0)*(2.0*x - 2.0*y));
G3 = l0*(z - zs) + l1*(-2.0*(29.254833995939044655187899479643*z + 64.0*z^3 - 54.62741699796951877488027093932*z*(x^2 + y^2))*(9.6568542494923796937200677348301*(x^2 + y^2)^2 - 1.0*(x^2 + y^2)*(27.31370849898475938744013546966*z^2 + 23.798989873223330704377076472156) + 14.627416997969522327593949739821*z^2 + 16.0*z^4 + 17.970562748477142633873882004991));
END;


